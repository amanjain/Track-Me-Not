{
  "manifest_version": 2,
  "name": "Track Me Not",
  "description": "Chrome extension for disabling tracking of your emails",
  "version": "0.2",
  "permissions": ["webRequest", "webRequestBlocking", "<all_urls>"],
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [{
    "run_at" : "document_start",
    "all_frames": true,
    "matches": ["*://mail.google.com/*"],
    "js": ["content.js"]
  }]
}
